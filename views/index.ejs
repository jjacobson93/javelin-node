				<li><a href="/studybuddies">Study Buddies</a></li>
				<li><a href="/changelog">Changelog</a></li>
			</ul>
		</div>
	</footer> -->
	
	<!--===============-->
	<!--=== SCRIPTS ===-->
	<!--===============-->

	<!-- jQuery -->
	<script src="/static/bower_components/jquery/dist/jquery.min.js"></script>
	<!-- Bootstrap -->
	<script src="/static/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
	<!-- Angular -->
	<script src="/static/bower_components/angular/angular.min.js"></script>
	<script src="/static/bower_components/angular-route/angular-route.min.js"></script>
	<script src="/static/bower_components/angular-resource/angular-resource.min.js"></script>
	<script src="/static/bower_components/angular-sanitize/angular-sanitize.min.js"></script>
	<script src="/static/bower_components/angular-animate/angular-animate.min.js"></script>
	<script src="/static/bower_components/angular-ui-bootstrap/dist/ui-bootstrap-tpls-0.11.0.min.js"></script>
	<script src="/static/bower_components/angular-ui-router/release/angular-ui-router.min.js"></script>
	<script src="/static/bower_components/angular-bootstrap-toggle-switch/angular-toggle-switch.min.js"></script>
	<script src="/static/bower_components/angular-scroll/angular-scroll.min.js"></script>
	<!-- Select2 -->
	<script src="/static/bower_components/select2/select2.min.js"></script>
	<script src="/static/bower_components/angular-ui-select2/src/select2.js"></script>
	<!-- FullCalendar -->
	<script src="/static/bower_components/jquery-ui/ui/minified/jquery-ui.custom.min.js"></script>
	<script src="/static/bower_components/fullcalendar/fullcalendar.js"></script>
	<script src="/static/bower_components/angular-ui-calendar/src/calendar.js"></script>

	<!-- Underscore -->
	<script src="/static/bower_components/underscore/underscore.js"></script>

	<!-- Moment -->
	<script src="/static/bower_components/moment/min/moment.min.js"></script>

	<!-- APP -->
	<script src="/static/js/app/main.js"></script>
	<script src="/static/js/app/util.js"></script>
	<!-- CONTROLLERS -->
	<script src="/static/js/app/controllers/main.js"></script>
	<script src="/static/js/app/controllers/dashboard.js"></script>
	<script src="/static/js/app/controllers/people.js"></script>
	<script src="/static/js/app/controllers/groups.js"></script>
	<script src="/static/js/app/controllers/events.js"></script>
	<script src="/static/js/app/controllers/orientation.js"></script>
	<script src="/static/js/app/controllers/tutoring.js"></script>
	<script src="/static/js/app/controllers/admin.js"></script>
	<script src="/static/js/app/controllers/messages.js"></script>
	<!-- DIRECTIVES -->
	<script src="/static/js/app/directives.js"></script>
	<!-- FILTERS -->
