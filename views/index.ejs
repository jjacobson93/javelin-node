	<!-- FACTORIES -->
	<script src="/static/js/app/factories.js"></script>
	<!-- SERVICES -->
	<script src="/static/js/app/services.js"></script>


	<script type="text/javascript">

	<% if ((typeof(flash) == "string" && flash !== "") || (typeof(flash) == "object" && flash && flash.message && flash.status)) { %>
	$(function() {
		notify("<%= (typeof(flash) != 'object') ? 'info' : flash.status || 'info' %>", "<%= (typeof(flash) == 'object') ? flash.message : flash %>");
	});
	<% } %>
	</script>
	<!--SCRIPTS END-->
</body>

</html>