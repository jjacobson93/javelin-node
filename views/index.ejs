	<link rel="stylesheet/less" href="/static/styles/flat-ui-switch.less">
	<link rel="stylesheet" href="/static/styles/pe-icon-7-stroke.css">
	<link rel="stylesheet" href="/static/styles/pe-icon-helper.css">
	<!-- <link rel="stylesheet/less" href="/static/styles/theme.less"> -->
	<link rel="stylesheet" href="/static/styles/colors.css">
	<link rel="stylesheet" href="/static/styles/fonts.css">
	<link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic' rel='stylesheet' type='text/css'>
	<link rel="stylesheet" href="/static/bower_components/animate.css/animate.min.css">
	<!-- LESS -->
	<link rel="stylesheet/less" href="/static/styles/variables.less">
	<link rel="stylesheet/less" href="/static/styles/style.less">
	<link rel="stylesheet/less" href="/static/styles/buttons.less">
	<link rel="stylesheet/less" href="/static/styles/badges.less">
	<link rel="stylesheet/less" href="/static/styles/animations.less">
	<link rel="stylesheet/less" href="/static/styles/typography.less">
	<link rel="stylesheet/less" href="/static/styles/login.less">
	<link rel="stylesheet/less" href="/static/styles/navigation.less">
	<link rel="stylesheet/less" href="/static/styles/marketing.less">
	<link rel="stylesheet/less" href="/static/styles/notifications.less">
	<link rel="stylesheet/less" href="/static/styles/form.less">
	<link rel="stylesheet/less" href="/static/styles/tables.less">
	<link rel="stylesheet/less" href="/static/styles/panels.less">
	<link rel="stylesheet/less" href="/static/styles/tabs.less">
	<link rel="stylesheet/less" href="/static/styles/people.less">
	<link rel="stylesheet/less" href="/static/styles/groups.less">
	<link rel="stylesheet/less" href="/static/styles/events.less">
	<!-- STYLES END -->

	<!-- DEV: Less -->
	<script src="/static/bower_components/less/dist/less-1.7.1.min.js"></script>

</head>


<div class="notification blue">
	<div class="overlay">
		<p class="lead"></p>
	</div>
</div>

<body ng-controller="MainController">
	<div class="navbar-javelin-top navbar-fixed-top">
		<a href="/" class="navbar-brand">
			<span class="javelin-brand"><img src="/static/images/javelin-icon-v2.svg"></span>
		</a>

		<nav class="main-nav">
			<ul class="nav navbar-nav navbar-right javelin-nav">
				<% if (authUser) { %>
				<li class="dropdown">
					<a a-prevent-default href="#" class="dropdown-toggle" data-toggle="dropdown">
						<img class="person-pic" src=""><%= authUser.first_name + ' ' + authUser.last_name %><span class="caret"></span>
					</a>
					<ul class="dropdown-menu">
						<li><a href="/logout"><span class="pe-7s-lock pe-lg"></span> Logout</a></li>
					</ul>
				</li>
				<% } else { %>
				<li>
					<a type="button" class="btn btn-teal navbar-btn" href="/#/login" style="margin-right:24px">Login</a>
				</li>
				<% } %>
			</ul>
		</nav>

		<% if (authUser) { %>
		<form class="navbar-form navbar-right main-search">
			<input class="form-control" style="width: 100%" ng-model="search.value" ng-change="search.change()">
			<span class="pe-7s-search form-control-feedback"></span>
			<span class="pe-7s-close-circle form-cancel-btn" ng-show="isSearching" ng-click="cancelSearch()"></span>
		</form>
		<% } %>
	</div>

	<% if (authUser) { %>
	<div class="navbar-javelin-side">
		<ul class="nav navbar-nav javelin-nav">
			<li ng-class="{active: $state.current.name.split('.')[0] == 'people'}">
				<a href="/#/people">
					<span class="green-border"><span class="pe-7s-users pe-3x"></span> People</span>
				</a>
			</li>
			<li ng-class="{active: $state.current.name.split('.')[0] == 'groups'}">
				<a href="/#/groups">
					<span class="red-border"><span class="pe-7s-network pe-3x"></span> Groups</span>
				</a>
			</li>
			<li ng-class="{active: $state.current.name.split('.')[0] == 'events'}">
				<a href="/#/events">
					<span class="purple-border"><span class="pe-7s-date pe-3x"></span> Events</span>
				</a>
			</li>
			<li ng-class="{active: $state.current.name.split('.')[0] == 'orientation'}">
				<a href="/#/orientation">
					<span class="teal-border"><span class="pe-7s-compass pe-3x"></span> Orientation</span>
				</a>
			</li>
			<li ng-class="{active: $state.current.name.split('.')[0] == 'tutoring'}">
				<a href="/#/tutoring">
					<span class="orange-border"><span class="pe-7s-notebook pe-3x"></span> Tutoring</span>
				</a>
			</li>
			<li ng-class="{active: $state.current.name.split('.')[0] == 'admin'}">
				<a href="/#/admin">
					<span class="yellow-border"><span class="pe-7s-tools pe-3x"></span> Admin</span>
				</a>
			</li>
		</ul>
	</div>
	<% } %>

	<div ui-view></div>

	<!-- <footer>
		<div class="container">
			<ul class="footer-links">
				<li class="pull-right"><span>Â© {{=request.now.year}} Jacobson and Varni</span></li>
